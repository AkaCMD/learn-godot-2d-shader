[sub_resource type="Shader" id="Shader_xoguk"]
code = "shader_type canvas_item;

uniform sampler2D sub_viewport;
uniform vec2 viewport_size = vec2(512,512);
varying vec2 world_pos;
uniform vec3 color: source_color;


void vertex() {
	world_pos = (MODEL_MATRIX * vec4(VERTEX, 0,1)).xy;
}

void fragment() {
	vec2 mask_uv = world_pos / viewport_size;
	vec4 viewport = texture(sub_viewport, mask_uv);
	COLOR.rgb = mix(COLOR.rgb, color, viewport.a);
}
"

